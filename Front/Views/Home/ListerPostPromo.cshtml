@{
    ViewBag.Title = "Justificatifs par Promo";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

@for (var i = 0; i <= 4; i++)
{

    <div class="panel-group col-lg-3">
        <h4>"@ViewBag.postsTD[@i].Texte"</h4>
        <h5>@ViewBag.postsTD[@i].Utilisateur_ID.Nom @ViewBag.postsTD[@i].Utilisateur_ID.Prenom, @ViewBag.postsTD[@i].Utilisateur_ID.Promo_ID.Libelle</h5>
        <p>Le : @ViewBag.postsTD[@i].Date</p>
        <p>Likes : @ViewBag.postsTD[@i].Likes | Dislikes : @ViewBag.postsTD[@i].Dislikes</p>
        <hr />
    </div>
}