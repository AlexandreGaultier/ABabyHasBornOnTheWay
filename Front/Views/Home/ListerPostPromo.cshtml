@{
    ViewBag.Title = "Justificatifs par Promo";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

@for (var i = 0; i <= @ViewBag.postsTDB1.Count - 1; i++)
{

    <div class="panel-group col-lg-3">
        <h4>"@ViewBag.postsTDB1[@i].Texte"</h4>
        <h5>@ViewBag.postsTDB1[@i].Utilisateur_ID.Nom @ViewBag.postsTDB1[@i].Utilisateur_ID.Prenom, @ViewBag.postsTDB1[@i].Utilisateur_ID.Promo_ID.Libelle</h5>
        <p>Le : @ViewBag.postsTDB1[@i].Date</p>
        <p>Likes : @ViewBag.postsTDB1[@i].Likes | Dislikes : @ViewBag.postsTDB1[@i].Dislikes</p>
        <hr />
    </div>
}

